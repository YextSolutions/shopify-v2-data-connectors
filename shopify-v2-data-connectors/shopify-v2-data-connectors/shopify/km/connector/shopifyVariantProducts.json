{
  "$id": "shopifyVariants",
  "$schema": "https://schema.yext.com/config/km/connector/v3",
  "displayName": "Shopify Variants",
  "entityType": "product",
  "source": {
    "sourceConfig": {
      "nativeSourceConfig": {
        "source": "shopify",
        "operation": "fetchVariants",
        "inputs": {
          "shopifyApiAccessToken": "${{shopifyApiAccessToken}}",
          "shopifyStoreName": "${{shopifyStoreName}}"
        }
      }
    },
    "selectors": [
      {
        "type": "JSON",
        "header": "product_id",
        "path": "product_id"
      },
      {
        "type": "JSON",
        "header": "id",
        "path": "id"
      },
      {
        "type": "JSON",
        "header": "title",
        "path": "title"
      },
      {
        "type": "JSON",
        "header": "price",
        "path": "price"
      },
      {
        "type": "JSON",
        "header": "sku",
        "path": "sku"
      },
      {
        "type": "JSON",
        "header": "position",
        "path": "position"
      },
      {
        "type": "JSON",
        "header": "inventory_policy",
        "path": "inventory_policy"
      },
      {
        "type": "JSON",
        "header": "fulfillment_service",
        "path": "fulfillment_service"
      },
      {
        "type": "JSON",
        "header": "inventory_management",
        "path": "inventory_management"
      },
      {
        "type": "JSON",
        "header": "option1",
        "path": "option1"
      },
      {
        "type": "JSON",
        "header": "created_at",
        "path": "created_at"
      },
      {
        "type": "JSON",
        "header": "updated_at",
        "path": "updated_at"
      },
      {
        "type": "JSON",
        "header": "taxable",
        "path": "taxable"
      },
      {
        "type": "JSON",
        "header": "barcode",
        "path": "barcode"
      },
      {
        "type": "JSON",
        "header": "grams",
        "path": "grams"
      },
      {
        "type": "JSON",
        "header": "weight",
        "path": "weight"
      },
      {
        "type": "JSON",
        "header": "weight_unit",
        "path": "weight_unit"
      },
      {
        "type": "JSON",
        "header": "inventory_item_id",
        "path": "inventory_item_id"
      },
      {
        "type": "JSON",
        "header": "inventory_quantity",
        "path": "inventory_quantity"
      },
      {
        "type": "JSON",
        "header": "old_inventory_quantity",
        "path": "old_inventory_quantity"
      },
      {
        "type": "JSON",
        "header": "requires_shipping",
        "path": "requires_shipping"
      },
      {
        "type": "JSON",
        "header": "admin_graphql_api_id",
        "path": "admin_graphql_api_id"
      },
      {
        "type": "JSON",
        "header": "option2",
        "path": "option2"
      },
      {
        "type": "JSON",
        "header": "image_id",
        "path": "image_id"
      }
    ]
  },
  "transforms": [
    {
      "addColumn": {
        "newColumnHeader": "currency",
        "columnValue": "USD"
      }
    }
  ],
  "mappings": [
    {
      "field": "inventoryQuantity",
      "header": "inventory_quantity",
      "subfieldPath": []
    },
    {
      "field": "shippingWeight",
      "header": "weight_unit",
      "subfieldPath": [
        {
          "name": "unit"
        }
      ]
    },
    {
      "field": "price",
      "header": "price",
      "subfieldPath": [
        {
          "name": "value"
        }
      ]
    },
    {
      "field": "parentProduct",
      "header": "product_id",
      "subfieldPath": []
    },
    {
      "field": "shippingWeight",
      "header": "weight",
      "subfieldPath": [
        {
          "name": "value"
        }
      ]
    },
    {
      "field": "entityId",
      "header": "id",
      "subfieldPath": []
    },
    {
      "field": "name",
      "header": "title",
      "subfieldPath": []
    },
    {
      "field": "sku",
      "header": "sku",
      "subfieldPath": []
    },
    {
      "field": "price",
      "header": "currency",
      "subfieldPath": [
        {
          "name": "currencyCode"
        }
      ]
    }
  ],
   "scheduleConfig": {
    "customSchedule": {
      "timeZone": "America/New_York",
      "startDate": "2022-10-22",
      "startTime": "00:00",
      "frequencyType": "DAILY"
    },
    "runMode": "DEFAULT"
